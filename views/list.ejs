  </section>
  <section class="search about bootstrap-iso">
    <div class="container-fluid">
      <div class="row ">
        <div class=" col search-menu">
          <div class="row sh">
            <h3 class="search-head">Search</h3>
            <i class="fas fa-search fa-3x"></i>
          </div>

          <form id="myForm" method="POST" action="/users/list">
            <div class="form-group row">
              <label class="col-2 col-form-label">Insect</label>
              <div class="col-4 drop-down">
                <select class="custom-select" id="myselect1" name="insect">
                  <option selected>Open this select menu</option>
                  <option value="bees">Bees</option>

                </select>
              </div>
              <label class="col-2 col-form-label">Scientific Name</label>
              <div class="col-4 drop-down">
                <select class="custom-select" id="myselect2" name="scientific_name">
                  <option selected>Open this select menu</option>
                  <option value="apiscerana">apis cerana</option>
                  <option value="apisdorsata">apis dorsata</option>
                  <option value="apisflorea">apis florea</option>
                  <option value="bwceratina">bw ceratina</option>
                </select>
              </div>
            </div>
            <div class="form-group row">
              <label class="col-2 col-form-label">Insect Color</label>
              <div class="col-4 drop-down">
                <select class="custom-select" id="myselect3" name="insect_color">
                  <option selected>Open this select menu</option>
                  <option value="blackandyellow">black and yellow</option>
                  <option value="blackandwhite">black and white</option>
                  <option value="blackwhiteandred">black white and red</option>

                </select>
              </div>
              <label class="col-2 col-form-label">Flower</label>
              <div class="col-4 drop-down">
                <select class="custom-select" id="myselect4" name="flower">
                  <option selected>Open this select menu</option>
                  <option value="dahlia">Dahlia</option>
                  <option value="portulacagrandiflora">Portulaca Grandiflora</option>
                  <option value="portulaca">Portulaca</option>

                </select>
              </div>
            </div>
            <div class="form-group row">
              <label class="col-2 col-form-label">Flower color</label>
              <div class="col-4 drop-down">
                <select class="custom-select" id="myselect5" name="flower_color">
                  <option selected>Open this select menu</option>
                  <option value="peach">peach</option>
                  <option value="pink">pink</option>
                  <option value="white">white</option>
                  <option value="yellow">yellow</option>

                </select>
              </div>
              <label class="col-2 col-form-label">Parameter</label>
              <div class="col-4 drop-down">
                <select class="custom-select" id="myselect6" name="parameter">
                  <option selected>Open this select menu</option>
                  <option value="1">One</option>
                  <option value="2">Two</option>
                  <option value="3">Three</option>
                </select>
              </div>
            </div>
            <div class="form-group row">
              <label class="col-2 col-form-label">Start date</label>
              <div class="col-4 drop-down">
                <!-- <form method="post" class="form2"> -->
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <div class="input-group-text">
                        <i class="fa fa-calendar fa"></i>
                      </div>
                    </div>
                    <!-- Date input -->
                    <input class="form-control date-input" id="date2" name="date2" placeholder="MM/DD/YYYY" type="text" />
                  </div>

                <!-- </form> -->
              </div>
              <label class="col-2 col-form-label">End date</label>
              <div class="col-4 drop-down ">

                <!-- <form method="post" class="form1"> -->
                  <div class="input-group">
                    <div class="input-group-prepend">
                      <div class="input-group-text">
                        <i class="fa fa-calendar fa"></i>
                      </div>
                    </div>
                    <!-- Date input -->
                    <input class="form-control date-input" id="date" name="date" placeholder="MM/DD/YYYY" type="text" />
                  </div>

                <!-- </form> -->
              </div>
            </div>
            <div class="form-group row">
              <div class="col-sm-10">
                <button type="submit" id="submitBtn" class="btn btn-outline-light btn-lg">Submit</button>
              </div>
            </div>
          </form>
        </div>
      </div>
    </div>
  </section>

